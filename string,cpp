#include <iostream>
using namespace std;
char printname(char name[]){
    cout<<"Hello "<<name<<endl;
}
int getlength(char name[]){
    int count=0;
    for(int i=0;name[i]!='\0';i++){
        count++;
}
return count;
}
void reversestring(char name[],length){
    int s=0;
    int e=len-1;
    while(s<e){
        swap(name[s++],name[e--]);
}
